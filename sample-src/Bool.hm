-- import Ord.hm
not(x) = case x
  { True() => False()
  | False() => True()
  };

and(x, y) = case x
  { True() => y
  | False() => False()
  };

or(x, y) = case x
  { True() => True()
  | False() => y
  };

eqBool(x, y) = case x
  { True() => y
  | False() => not(y)
  };

ordBool(x, y) = case x
  { False() => case y
    { False() => EQ()
    | True() => LT()
    }
  | True() => case y
    { False() => GT()
    | True() => EQ()
    }
  };
