data List(a) of Nil() | Cons(a, List(a));

map(f, xs) = case xs
  { Nil() => Nil()
  | Cons(y, ys) => Cons( f(y), map(f, ys))
  };

itemPrinter(x) = out_int(x) >> out_string("\t");

setIt(arr,pos) = set(arr, pos, [1,2,3]);


main() = let arr = makeArray(10, Nil()) in
  setIt(arr,3) >> map(itemPrinter, (get(arr,3)));

